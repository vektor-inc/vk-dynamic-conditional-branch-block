{
	"apiVersion": 2,
	"name": "vk-blocks/dynamic-if",
	"title": "Dynamic If",
	"category": "theme",
	"textdomain": "vk-dynamic-if-block",
	"attributes": {
		"selectCondition": {
			"type": "string",
			"default": "none"
		},
		"ifPageType": {
			"type": "string",
			"default": "none"
		},
		"ifPostType": {
			"type": "string",
			"default": "none"
		},
		"userRole": {
			"type": "array",
			"default": []
		},
		"customFieldName": {
			"type": "string",
			"default": ""
		},
		"customFieldRule": {
			"type": "string",
			"default": "valueExists"
		},
		"customFieldValue": {
			"type": "string",
			"default": ""
		},
		"exclusion": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false,
		"innerBlocks": true
	},
	"editorScript": "url:./build/index.js",
	"editorStyle": "url:./build/editor.css"
}
